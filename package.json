{
  "name": "aoc",
  "author": "atheius",
  "version": "1.1.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "tap": {
    "node-arg": [
      "--no-warnings",
      "--experimental-loader",
      "@istanbuljs/esm-loader-hook"
    ],
    "branches": 85,
    "lines": 85,
    "functions": 85,
    "statements": 85
  },
  "bin": {
    "aoc": "bin/aoc.js"
  },
  "scripts": {
    "test": "tap ./src/challenges/**/*.test.js --no-cov",
    "test:watch": "tap ./src/challenges/**/*.test.js --watch",
    "test:coverage": "tap ./src/challenges/**/*.test.js",
    "benchmark": "node ./src/benchmark.js"
  },
  "license": "ISC",
  "dependencies": {
    "algebra.js": "^0.2.6",
    "arg": "^5.0.2",
    "chalk": "^5.1.2",
    "dotenv": "^16.0.3",
    "jsdom": "^20.0.3",
    "just-clone": "^6.1.1",
    "lodash": "^4.17.21",
    "log-update": "^5.0.1",
    "node-fetch": "^3.3.0",
    "turndown": "^7.1.1"
  },
  "devDependencies": {
    "@istanbuljs/esm-loader-hook": "^0.2.0",
    "@types/algebra.js": "^0.2.2",
    "tap": "^16.3.2"
  }
}
